<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.sec.android.app.launcher">

    <home>
	<homeGridInfo default="4x5"/>
		<!--  screen [1]  -->
        <!-- appwidget : Weather -->
        <appwidget 
            screen="0" 
            packageName="com.sec.android.daemonapp" 
            className="com.sec.android.daemonapp.appwidget.WeatherAppWidget2x1" 
            x="0" 
            y="2" 
            spanX="4" 
            spanY="1" />

        <!-- appwidget : Google -->
        <appwidget 
            screen="0" 
            packageName="com.google.android.googlequicksearchbox" 
            className="com.google.android.googlequicksearchbox.SearchWidgetProvider" 
            x="0" 
            y="3" 
            spanX="4" 
            spanY="1" />

        <!-- Galaxy Store -->
        <favorite 
            screen="0" 
            packageName="com.sec.android.app.samsungapps" 
            className="com.sec.android.app.samsungapps.SamsungAppsMainActivity" 
            x="0" 
            y="4" />

        <!-- Google Play Store -->
        <favorite 
            screen="0" 
            packageName="com.android.vending" 
            className="com.android.vending.AssetBrowserActivity" 
            x="1" 
            y="4" />

        <!-- folder : Google -->
        <folder 
            screen="0" 
            title="Google" 
            x="2" 
            y="4">
            <!-- Google -->
            <favorite 
                screen="0" 
                packageName="com.google.android.googlequicksearchbox" 
                className="com.google.android.googlequicksearchbox.SearchActivity" />
            <!-- Chrome -->
            <favorite 
                screen="1" 
                packageName="com.android.chrome" 
                className="com.google.android.apps.chrome.Main" />
            <!-- Gmail -->
            <favorite 
                screen="2" 
                packageName="com.google.android.gm" 
                className="com.google.android.gm.ConversationListActivityGmail" />
            <!-- Maps -->
            <favorite 
                screen="3" 
                packageName="com.google.android.apps.maps" 
                className="com.google.android.maps.MapsActivity" />
            <!-- YouTube -->
            <favorite 
                screen="4" 
                packageName="com.google.android.youtube" 
                className="com.google.android.youtube.app.honeycomb.Shell$HomeActivity" />
            <!-- Drive -->
            <favorite 
                screen="5" 
                packageName="com.google.android.apps.docs" 
                className="com.google.android.apps.docs.app.NewMainProxyActivity" />
            <!-- Google Play Music -->
            <favorite 
                screen="6" 
                packageName="com.google.android.music" 
                className="com.android.music.activitymanagement.TopLevelActivity" />
            <!-- Google Play Movies & TV -->
            <favorite 
                screen="7" 
                packageName="com.google.android.videos" 
                className="com.google.android.youtube.videos.EntryPoint" />
            <!-- Duo -->
            <favorite 
                screen="8" 
                packageName="com.google.android.apps.tachyon" 
                className="com.google.android.apps.tachyon.MainActivity" />
            <!-- Photos -->
            <favorite
                screen="9" 
                packageName="com.google.android.apps.photos" 
                className="com.google.android.apps.photos.home.HomeActivity" />
        </folder>

       <!-- folder : Microsoft Apps -->
        <folder 
            screen="0" 
            title="Microsoft Apps" 
            x="3" 
            y="4">
            <!-- Office Mobile -->
            <favorite 
                screen="0" 
                packageName="com.microsoft.office.officehubrow" 
                className="com.microsoft.office.officesuite.OfficeSuiteActivity" />
            <!-- OneDrive -->
            <favorite 
                screen="1" 
                packageName="com.microsoft.skydrive" 
                className="com.microsoft.skydrive.MainActivity" />
            <!-- LinkedIn -->
            <favorite 
                screen="2" 
                packageName="com.linkedin.android" 
                className="com.linkedin.android.authenticator.LaunchActivity" />
        </folder> 
       <!-- My Singtel -->
        <favorite 
            screen="1" 
            packageName="com.singtel.mysingtel" 
            className="com.singtel.mysingtel.MainActivity"
            x="0" 
            y="0" />
    </home>


    <hotseat>
        <!-- Phone -->
        <favorite 
            screen="0" 
            packageName="com.samsung.android.dialer" 
            className="com.samsung.android.dialer.DialtactsActivity" />

        <!-- Messages -->
        <favorite 
            screen="1" 
            packageName="com.samsung.android.messaging" 
            className="com.android.mms.ui.ConversationComposer" />
			
        <!-- Internet -->
        <favorite 
            screen="2" 
            packageName="com.sec.android.app.sbrowser" 
            className="com.sec.android.app.sbrowser.SBrowserMainActivity" />
			
        <!-- Camera -->
        <favorite 
            screen="3" 
            packageName="com.sec.android.app.camera" 
            className="com.sec.android.app.camera.Camera" />
    </hotseat>

</favorites>
